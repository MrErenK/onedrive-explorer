    "@types/busboy": "^1.5.4",
    "@types/cli-progress": "^3.11.6",
    "busboy": "^1.6.0",
    "clsx": "^2.1.1",
    "express": "^4.21.0",
    "framer-motion": "^11.5.4",
    "next": "^14.2.12",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.5.2",
    "tsx": "^4.19.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
